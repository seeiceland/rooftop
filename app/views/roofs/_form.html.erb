<%= simple_form_for(@rooftop) do |f| %>
  <div class="form-inputs">
    <%= f.input :name %>
     <%= f.input :location %>
      <%= f.input :price %>
       <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  </div>
  <div class="form-actions" class ="btn btn-primary">
    <%= f.button :submit %>
  </div>
<%end%>
